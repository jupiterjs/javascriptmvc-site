c({"name": "DocumentJS.tags.constructor", "type": "class", "comment": "<p>Documents a constructor function and its parameters. </p>\n\n<h3>Example:</h3>\n\n<pre><code class='javascript'>/*\n * @class Customer \n * @constructor\n * Creates a new customer.\n * @param {String} name\n */\n var Customer = function(name) {\n    this.name = name;\n }</code></pre> ", "parents": ["DocumentJS.tags"], "src": "documentjs/tags/constructor.js", "line": 1, "children": []})